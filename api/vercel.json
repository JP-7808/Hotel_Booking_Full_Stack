{
    "version": 2,
    "builds": [
      {
        "src": "api/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/api/index.js"
      }
    ],
    "functions": {
        "api/*.js": {
        "runtime": "nodejs14.x"  
        }
    }
  }
  